{
  "name": "express-starter",
  "version": "1",
  "description": "boilerplate for starting express apis",
  "main": "index.js",
  "repository": "git@github.com:drericrobinson/express-starter.git",
  "author": "Eric Robinson <soyecr+dr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=testing jest --forceExit --detectOpenHandles  --silent",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "restart": "rimraf dist && yarn build && yarn start",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "client": "cd client && yarn start",
    "server": "ndb nodemon --exec yarn restart"
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.6.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "23.6.0",
    "ndb": "^1.0.43",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test-db-setup.js"
    ],
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  }
}
